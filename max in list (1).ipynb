    "def max_value():\n",
    "    a = [1,3,4,6,10]\n",
    "    Max = a[0]\n",
    "    for i in a:\n",
    "        if (i > Max):\n",
    "            Max = i\n",
    "    print(f\"The largest number from the {a} is {Max}.\")"
   ]
